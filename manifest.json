{
  "name": "YouTube - Support Independent Media",
  "description": "Support Independent YouTube channels that tell truth about the war in Ukraine - fight Putin's propaganda",
  "version": "1.03",
  "manifest_version": 3,
  "permissions": ["storage"],
  "icons": {
    "128": "/images/img128_2.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "css": ["main.css"],
      "js": ["script.js", "scriptPlaylist.js"]
    }
  ],

  "action": {
      "default_title": "Open YouTube Channels",
      "default_popup": "popup.html"
  }
}
{
  "name": "Support free media on YouTube",
  "description": "Support Independent YouTube channels - fight propaganda",
  "version": "1.11",
  "manifest_version": 3,
  "permissions": ["storage"],
  "icons": {
    "128": "/images/img128_6.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "css": ["popup.css", "contentHighlight.css", "contentNotifications.css"],
      "js": ["config.js", "runByChannel.js", "runPlaylist.js", "runHighlight.js", "runNotifications.js", "runReport.js"]
    }
  ],

  "action": {
      "default_title": "Stop war - support free media",
      "default_popup": "popup.html"
  }
}
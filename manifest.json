{
  "name": "Stop war - support free media on YouTube",
  "description": "Support Independent YouTube channels telling truth about the war in Ukraine - fight Putin's propaganda",
  "version": "1.06",
  "manifest_version": 3,
  "permissions": ["storage", "tabs"],
  "icons": {
    "128": "/images/img128_6.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "css": ["main.css"],
      "js": ["runByChannel.js", "runPlaylist.js"]
    }
  ],

  "action": {
      "default_title": "Stop war - support free media",
      "default_popup": "popup.html"
  }
}